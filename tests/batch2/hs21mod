variables

x1 in [2, 50];
x2 in [-50, 50];
x3 in [-1e9, 50];
x4 in [2, 1e9];
x5 in [-1e9, 1e9];
x6 in [-1e9, 0];
x7 in [0, 1e9];

minimize ((((((((0.01000000000000001*x1^2)+(0.01000000000000001*x3^2))+(0.01000000000000001*x5^2))+(0.01000000000000001*x6^2))+x2^2)+x4^2)+x7^2)+-100)

constraints

(((10*x1)-x2)-10)>=0;

end